    <div style="text-align:right">
        <a href="/Administrador/Login" id="regresar">Cerrar sesion</a>
    </div>
    <div class="container-fluid cew-9">
        <div class="row">
            <div class="col">
                <b><label for="periodoacademicoCtxt">Periodo Academico</label></b>
                <div class="form-group">
                    <input type="number" placeholder="2023-1" style="width : 300px; heigth : 5px" id="reportartxt" required />
                </div>
            </div>
            <div class="col">
                <b><label for="programa">Programa</label></b>
                <br>
                <select name="programa" id="programa">
                    <option value="ingenieria">Ingenieria de Software</option>
                    <option value="maestria">Maestria en Gestion TI</option>
                    <option value="si">Especializacion SI</option>
                    <option value="medioambiente">Maestria en Medio Ambiente</option>
                </select>
            </div>
        </div>
        <br>
        <br>
        <table class="table">
            <thead>
                <tr>
                    <th scope="col">FECHA</th>
                    <th scope="col">SALA O AULA</th>
                    <th scope="col">ASIGNATURA</th>
                    <th scope="col">CODIGO ASIGNATURA</th>
                    <th scope="col">OPCION</th>
                </tr>
            </thead>
            <tbody>
                <tr class="text-center">
                    <td><input type="date" style="width : 140px; heigth : 5px" id="fechatxt" required /></td>
                    <td><input type="number" placeholder="5-210" col-sm-1" style="width : 80px; heigth : 5px" id="numdoctxt" required /></td>
                    <td><input type="text" placeholder="Construccion de Software" col-sm-1" style="width : 220px; heigth : 5px" id="numdoctxt" required /></td>
                    <td><input type="number" placeholder="3456789" style="width : 120px; heigth : 5px" id="numdoctxt" required /></td>
                    <td><input type="button" class="btn btn-outline-primary" value="BUSCAR"></td>
                </tr>
            </tbody>
        </table>
    </div>
    <br>
</div>